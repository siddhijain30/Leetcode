class Solution {
public:
    int findDuplicate(vector<int>& nums) {
        //time complexity : 0(n^2)
        for(int i=0; i<nums.size();i++){
            for(int j=i+1;j<nums.size();j++){
                if(nums[i]==nums[j])
                return nums[i];
                }
            }
        return 0;

        //time complexity : 0(n)
        for(int i=0; i<nums.size(); i++) {
            int idx = abs(nums[i]);
            if(nums[idx] < 0)
                return idx;
            nums[idx] *= -1;
        }
        return 0;
    }
};
